<% provide :title, @title %>

<div class="row">
  <aside class="col-md-4">
    <section class="stats">
      <%= render "stats" %>
    </section>
  </aside>
  <div class="col-md-8">
    <% if @users.any? %>
      <ul class="users follow">
        <%= render @users %>
      </ul>
    <% end %>
  </div>
</div>
